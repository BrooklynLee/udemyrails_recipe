<%= render 'shared/pages_title' , title: 'All Chefs' %>

<ul class="nav nav-tabs">
  <li class="active">
    <%= link_to "Chefs", chefs_path %>
  </li>
  <li>
    <%= link_to "Recipes", recipes_path %>
  </li>
</ul>

<%= will_paginate %>

<% @chefs.each do |chef| %>
  
  <div class="row">
    
    <div class="col-md-4 col-md-offset-4 center">
    
        <%= link_to gr_to( chef, size: 200), chef_path(chef) %>
        <h5>Chef name: <%= chef.chefname %></h5>
    
    </div>
  

    
<% end %>

